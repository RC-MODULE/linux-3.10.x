config RCM_VDU_VIDEO
	tristate "RCM VDU video output support"
	depends on VIDEO_V4L2
	select FB_RCM_VDU_CORE
	default n
	---help---
	  Support for V4L2 interface to RCM VDU.

config RCM_VPU_CODA980
	tristate "RCM VPU CODA980 video codec driver"
	default n
	---help---
	  RCM VPU CODA980 (Chips Media) video codec driver.
